{
    "$schema": "https://schema.tauri.app/config/2",
    "productName": "simplelist",
    "version": "0.0.2",
    "identifier": "com.simpleist.app",
    "build": {
        "frontendDist": "../src"
    },
    "app": {
        "withGlobalTauri": true,
        "windows": [{
            "title": "极简打卡",
            "width": 800,
            "height": 600
        }],
        "security": {
            "csp": null
        }
    },
    "bundle": {
        "active": true,
        "targets": "all",
        "icon": [
            "icons/32x32.png",
            "icons/128x128.png",
            "icons/128x128@2x.png",
            "icons/icon.icns",
            "icons/icon.ico"
        ]
    }
}